<div class="pac-card" id="pac-card">
  <div>

    <div id="type-selector" class="pac-controls">

    </div>
  </div>
  <div id="pac-container">
    <input id="pac-input" type="text" placeholder="Enter a location">
  </div>
</div>
<div id="map"></div>
<div class="add-toilet">
  <h2>Add a toilet</h2>
</div>
<form method="POST" class="add-form" id="form" action="/map" enctype="multipart/form-data">
  Location Name
  <input type="text" name="name"> Descritption
  <input type="text" name="description"> Foto:
  <input type="file" name="image">
  <input type="text" name="lat" id="lat" style="visibility:hidden">
  <input type="text" name="lng" id="lng" style="visibility:hidden">
  <button class="btn">Submit</button>
</form>
<script>let places = <%- JSON.stringify(places) %></script>
<script src="./javascripts/map.js" type="text/javascript"></script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=<%-API_MAP%>&callback=initMap&libraries=places">
</script>